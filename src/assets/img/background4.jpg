<script>

	$(document).ready(function () {
		var userLocationSet = false;
		var allcookies = document.cookie;

		if (allcookies.indexOf("user_location") >= 0) {
			userLocationSet = true;
		}

		if (userLocationSet == false) {
			//$("#location_selection").modal({ backdrop: 'static', keyboard: false }, 'show');
		}



		$("#citySelection").on("change", function () {

			$("#branches option").each(function () {
				$(this).remove();
			});
			$("#branches").attr("disabled", false);



			if (this.value == "all") {
				let options = '<option value="all_all">All branches</option>';
				$("#branches").append(options);
				$("#branches").attr("disabled", true);
			} else {
				$("#branches option").each(function () {
					$(this).remove();
				});
				let urlVal = "https://www.achieversit.com/index.php/Home/getBranchByCity?city=" + this.value;
				$.ajax({
					url: urlVal, success: function (result) {
						$("#branches").append(result);
					}
				});
			}

		});

		var now = new Date();
  		var time = now.getTime();
  		var expireTime = time + 1000*3600; // SET COOKIE FOR 1 hour
  		now.setTime(expireTime);
  		
		$("#setDefaultData").on("click", function () {
			document.cookie = "user_location=all_all;expires="+now.toUTCString()+";path=/";
			location.reload();
		});

		$("#loc_save_close").on("click", function () {
			let cityName = $("#citySelection option:selected").val();
			let branchName = $("#branches option:selected").val();
			branchName = (branchName == "all") ? "all" : branchName;
			branchName = (cityName == "all") ? "all" : branchName;
			// alert(cityName +" " +branchName);
			document.cookie = "user_location=" + cityName + "_" + branchName+";expires="+now.toUTCString()+";path=/";
			$("#location_selection").modal("hide");
			location.reload();
		});

		$(".tabs").click(function () {

			$(".tabs").removeClass("active");
			$(".tabs h6").removeClass("font-weight-bold");
			$(".tabs h6").addClass("text-muted");
			$(this).children("h6").removeClass("text-muted");
			$(this).children("h6").addClass("font-weight-bold");
			$(this).addClass("active");

			current_fs = $(".active");

			next_fs = $(this).attr('id');
			next_fs = "#" + next_fs + "1";

			$("fieldset").removeClass("show");
			$(next_fs).addClass("show");

			current_fs.animate({}, {
				step: function () {
					current_fs.css({
						'display': 'none',
						'position': 'relative'
					});
					next_fs.css({
						'display': 'block'
					});
				}
			});
		});
	});
	//New menu styling - 19-09-2021 -Murali P
	$('.dropdown-menu a.dropdown-toggle').on('click', function (e) {

		if (!$(this).next().hasClass('show')) {
			$(this).parents('.dropdown-menu').first().find('.show').removeClass('show');
		}
		var $subMenu = $(this).next('.dropdown-menu');
		$subMenu.toggleClass('show');

		$(this).parents('li.nav-item.dropdown.show').on('hidden.bs.dropdown', function (e) {
			$('.dropdown-submenu .show').removeClass('show');
		});
		return false;
	});

</script>
